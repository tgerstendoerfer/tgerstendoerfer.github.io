---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title type="text" xml:lang="de">gerstendoerfer.ch</title>
	<link type="application/atom+xml" href="http://gerstendoerfer.ch/feed.xml" rel="self"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://gerstendoerfer.ch/</id>
	<author>
		<name>Thomas Gerstendörfer</name>
	</author>
	<rights>Copyright (c) 2013 Thomas Gerstendörfer. All rights reserved.</rights>
	
	{% for post in site.posts limi:20 %}
	<entry>
		<title>{{ post.title }}</title>
		<link href="http://gerstendoerfer.ch{{ post.url }}"/>
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>http://gerstendoerfer.ch{{ post.id }}</id>
		<content type="html">{{ post.content | xml_escape }}</content>
	</entry>
	{% endfor %}
</feed>
